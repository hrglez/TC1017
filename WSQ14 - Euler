#include <iostream>
#include <cmath>
using namespace std;


double calculate_e(double x){
  double e,y;
  if(x==0){
    e=1;
    return e;
    }
    y= 1+(1/x);
    e=pow(y,x);
 return e;
}

int main(){
double n;
double e;
cout<<"Enter the accuracy for calculatig the number euler: ";
cin>>n;
e=calculate_e(n);
cout<<"Euler is: "<<e<<endl;
return 0;
}
